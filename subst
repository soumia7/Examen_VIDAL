                    <form action="recherche/" method="post" class="clearFix" id="search_form">
    <input type="hidden" id="inp_click_acp" name="search[click_acp]" value="0">
    <div id="search_error" style="display:none:opacity:0;filter:opacity(alpha=0);"></div>
    <div class="select first left">
        <select name="search[where]" id="sel_search_s">
            <option value="">Dans tout le site</option>
            <option value="1" >Les monographies</option>
