    <script type='text/javascript'>
        
        var autoFill = 'Doliprane, asthme, aspirine, amoxicilline...';
        Event.observe("search_form", "submit", function(e){
            var value = $("inp_search_q").value;
            if (value == autoFill)
            {
                e.stop();
                searchError("Veuillez indiquer votre recherche.");
            }
            else if (value.length < 3)
            {
                e.stop();
                searchError("Veuillez saisir au moins 3 caract&egrave;res pour votre recherche.");
            }
        });

        function searchError(pMessage) {
            var b = $("search_error");
            b.innerHTML = pMessage;
            $fadeIn(b,.3);
            setTimeout(function(){$fadeOut(b,.3);},2500);
        }

        AutoFillPlugin.applyTo(document.getElementById('inp_search_q'), autoFill);

        YUI().use('autocomplete', 'autocomplete-highlighters', function (Y)
        {
			Y.one('#inp_search_q').plug(Y.Plugin.AutoComplete, {
				resultHighlighter: 'phraseMatch',
				resultListLocator: 'responses',
				resultTextLocator: 'value',
				minQueryLength:3,
				on:
				{
					query:function(){$fadeIn($one("#search_form .loading"),.3);},
					results:function(){$fadeOut($one("#search_form .loading"),.3);},
					clear:function(){$fadeOut($one("#search_form .loading"),.3);},
					select:function(e){$("inp_search_q").value = e.result.text;$("inp_click_acp").value = 1;$("search_form").submit();}
				},
				source: 'statique/autocomplete/application:main/is_backoffice:/form_name:search/input_name:q/q:{query}/'
			});
        });


        $("sel_search_h").observe("change", function(e) {
            var v = this.value;
            if (v) {
                window.location = "/"+v;
            }
        });
        
        </script>




</form>
                    <div id="alpha_search">
                        <span>Recherche alphab&eacute;tique :</span>
                        <ul class="clearFix">
                            <li><a href="recherche/alphabetique/start:1-9/">1-9</a></li><li><a href="recherche/alphabetique/start:a/">a</a></li><li><a href="recherche/alphabetique/start:b/">b</a></li><li><a href="recherche/alphabetique/start:c/">c</a></li><li><a href="recherche/alphabetique/start:d/">d</a></li><li><a href="recherche/alphabetique/start:e/">e</a></li><li><a href="recherche/alphabetique/start:f/">f</a></li><li><a href="recherche/alphabetique/start:g/">g</a></li><li><a href="recherche/alphabetique/start:h/">h</a></li><li><a href="recherche/alphabetique/start:i/">i</a></li><li><a href="recherche/alphabetique/start:j/">j</a></li><li><a href="recherche/alphabetique/start:k/">k</a></li><li><a href="recherche/alphabetique/start:l/">l</a></li><li><a href="recherche/alphabetique/start:m/">m</a></li><li><a href="recherche/alphabetique/start:n/">n</a></li><li><a href="recherche/alphabetique/start:o/">o</a></li><li><a href="recherche/alphabetique/start:p/">p</a></li><li><a href="recherche/alphabetique/start:q/">q</a></li><li><a href="recherche/alphabetique/start:r/">r</a></li><li><a href="recherche/alphabetique/start:s/">s</a></li><li><a href="recherche/alphabetique/start:t/">t</a></li><li><a href="recherche/alphabetique/start:u/">u</a></li><li><a href="recherche/alphabetique/start:v/">v</a></li><li><a href="recherche/alphabetique/start:w/">w</a></li><li><a href="recherche/alphabetique/start:x/">x</a></li><li><a href="recherche/alphabetique/start:y/">y</a></li><li><a href="recherche/alphabetique/start:z/">z</a></li>                        </ul>
