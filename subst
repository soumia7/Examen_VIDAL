    <script type='text/javascript'>
        
        var autoFill = 'Doliprane, asthme, aspirine, amoxicilline...';
        Event.observe("search_form", "submit", function(e){
            var value = $("inp_search_q").value;
            if (value == autoFill)
            {
                e.stop();
                searchError("Veuillez indiquer votre recherche.");
            }
            else if (value.length < 3)
            {
                e.stop();
                searchError("Veuillez saisir au moins 3 caract&egrave;res pour votre recherche.");
            }
        });

        function searchError(pMessage) {
            var b = $("search_error");
            b.innerHTML = pMessage;
            $fadeIn(b,.3);
            setTimeout(function(){$fadeOut(b,.3);},2500);
        }

        AutoFillPlugin.applyTo(document.getElementById('inp_search_q'), autoFill);

        YUI().use('autocomplete', 'autocomplete-highlighters', function (Y)
        {
			Y.one('#inp_search_q').plug(Y.Plugin.AutoComplete, {
				resultHighlighter: 'phraseMatch',
				resultListLocator: 'responses',
				resultTextLocator: 'value',
				minQueryLength:3,
				on:
				{
					query:function(){$fadeIn($one("#search_form .loading"),.3);},
					results:function(){$fadeOut($one("#search_form .loading"),.3);},
					clear:function(){$fadeOut($one("#search_form .loading"),.3);},
					select:function(e){$("inp_search_q").value = e.result.text;$("inp_click_acp").value = 1;$("search_form").submit();}
				},
				source: 'statique/autocomplete/application:main/is_backoffice:/form_name:search/input_name:q/q:{query}/'
			});
        });


        $("sel_search_h").observe("change", function(e) {
            var v = this.value;
            if (v) {
                window.location = "/"+v;
            }
        });
        
        </script>




</form>
